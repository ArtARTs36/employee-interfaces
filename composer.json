{
    "name": "artarts36/employee-interfaces",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "ArtARTs36",
            "email": "temicska99@mail.ru"
        }
    ],
    "autoload": {
        "psr-4": {
            "ArtARTs36\\EmployeeInterfaces\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ArtARTs36\\EmployeeInterfaces\\Tests\\": "Tests"
        }
    },
    "require": {
        "php": ">=7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "squizlabs/php_codesniffer": "3.*"
    },
    "description": "Employee interfaces",
    "scripts": {
        "lint": [
            "./vendor/bin/phpcs --standard=PSR2 src/"
        ],
        "test": [
            "./vendor/bin/phpunit -v --coverage-text --colors=always --configuration phpunit.xml"
        ]
    }
}
